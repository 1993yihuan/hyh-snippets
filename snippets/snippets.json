{
	"Print to console": {
		"prefix": "c",
		"body": [
			"console.log('$1');"
		],
		"description": "控制台打印"
	},
	"Print to alert": {
		"prefix": "a",
		"body": [
			"alert('$1');"
		],
		"description": "弹窗警告"
	},
	"Print to function": {
		"prefix": "f",
		"body": [
			"($1) => {",
			"    $2",
			"};"
		],
		"description": "箭头函数"
	},
	"jsx file": {
		"prefix": "jsx",
		"body": [
			"import React, { Component } from 'react';",
			"",
			"class ${1:componentName} extends Component {",
			"    return(",
			"        render() {",
			"            <div>",
			"                $2",
			"            </div>",
			"        }",
			"    )",
			"}",
			"export default ${1:componentName};"
		],
		"description": "jsx文件主要內容"
	}
}